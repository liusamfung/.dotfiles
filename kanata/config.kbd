;; o Navigation
;; y .espaces
;;  a windows
;;  i sesion
;;  u tabs
(defalias
  ' (multi f23 (tap-hold $tap-time $hold-time ' ralt))
  , (multi f24 (tap-hold $tap-time $hold-time , lctl))
  . (multi f24 (tap-hold $tap-time $hold-time . lsft))
  p (multi f24 (tap-hold $tap-time $hold-time p lmet))

  g (multi f24 (tap-hold $tap-time $hold-time g rmet))
  c (multi f24 (tap-hold $tap-time $hold-time c rsft))
  r (multi f24 (tap-hold $tap-time $hold-time r lctl))
  l (multi f24 (tap-hold $tap-time $hold-time l ralt))
  
   ;; Si lo que quieres es que apenas pulsando otra tecla se accione el layer una -press
   ;; Rsft_lyr (tap-hold-press  150 10 @hrow_sc (layer-toggle shift))
   Rsft_lyr (tap-hold-press  $tap-time $hold-time ret (layer-toggle shift))
   Lsft_lyr (tap-hold-press  $tap-time $hold-time bspc (layer-toggle shift))
   test_lyr (tap-hold-release-timeout $tap-time $hold-time e (layer-toggle shift) a )

  ;; Sublayerrrrr #################################################################
   msubl (tap-hold-press $subl-time $subl-time XX (layer-while-held sublayers))
   win_slyr (tap-hold-press $subl-time $subl-time x (layer-while-held WindowsSubLayer))
    spa_slyr (tap-hold-press $subl-time $subl-time y (layer-while-held SpaceSubLayer))

   nav_lyr (tap-hold $tap-time $hold-time a (layer-toggle navigationLayer))
   lnch_lyr (tap-hold $tap-time $hold-time o (layer-toggle apps))
   num_lyr (tap-hold $tap-time $hold-time q (layer-toggle number))
   ray_lyr (tap-hold $tap-time $hold-time n (layer-toggle raycastUtilyExtension))
   spa_lyr (tap-hold $tap-time $hold-time y (layer-toggle SpaceLayer))
   win_lyr (tap-hold $tap-time $hold-time x (layer-toggle WindowsLayer))
   tab_lyr (tap-hold $tap-time $hold-time i (layer-toggle TabsLayer))

   app_lyr (tap-hold $tap-time $hold-time spc (layer-toggle EspecificAppsLayer))

   sys_lyr (tap-hold $tap-time $hold-time @: (layer-toggle systemLayer))
   ses_lyr (tap-hold $tap-time $hold-time u (layer-toggle sesionLayer))
)
